{
  "auxInteractionFields": [
    {
      "fieldName": "aux_date_created",
      "fieldType": "DateTime"
    },
    {
      "fieldName": "aux_id",
      "fieldType": "String",
      "source": {
        "csvCol": 0
      }
    },
    {
      "fieldName": "aux_clinical_data",
      "fieldType": "String",
      "source": {
        "csvCol": 10
      }
    }
  ],
  "auxGoldenRecordFields": [
    {
      "fieldName": "aux_date_created",
      "fieldType": "DateTime"
    },
    {
      "fieldName": "aux_auto_update_enabled",
      "fieldType": "Bool",
      "default": "true"
    },
    {
      "fieldName": "aux_id",
      "fieldType": "String"
    }
  ],
  "additionalNodes": [
    {
      "nodeName": "SourceId",
      "fields": [
        {
          "fieldName": "facility",
          "fieldType": "String",
          "source": {
            "csvCol": 8
          }
        },
        {
          "fieldName": "patient",
          "fieldType": "String",
          "source": {
            "csvCol": 9
          }
        }
      ]
    }
  ],
  "demographicFields": [
    {
      "fieldName": "given_name",
      "fieldType": "String",
      "source": {
        "csvCol": 1
      },
      "indexGoldenRecord": "@index(exact,trigram)",
      "indexInteraction": "@index(exact,trigram)"
    },
    {
      "fieldName": "family_name",
      "fieldType": "String",
      "source": {
        "csvCol": 2
      },
      "indexGoldenRecord": "@index(exact,trigram)",
      "indexInteraction": "@index(exact,trigram)"
    },
    {
      "fieldName": "gender",
      "fieldType": "String",
      "source": {
        "csvCol": 3
      },
      "indexGoldenRecord": "@index(exact)"
    },
    {
      "fieldName": "dob",
      "fieldType": "String",
      "source": {
        "csvCol": 4
      }
    },
    {
      "fieldName": "city",
      "fieldType": "String",
      "source": {
        "csvCol": 5
      },
      "indexGoldenRecord": "@index(trigram)"
    },
    {
      "fieldName": "phone_number",
      "fieldType": "String",
      "source": {
        "csvCol": 6
      },
      "indexGoldenRecord": "@index(exact,trigram)"
    },
    {
      "fieldName": "national_id",
      "fieldType": "String",
      "source": {
        "csvCol": 7
      },
      "indexGoldenRecord": "@index(exact)",
      "indexInteraction": "@index(exact)"
    },
    {
      "fieldName": "my_golden_id_a",
      "fieldType": "String",
      "source": {
        "generate": {
          "func": "AppUtils::autoGenerateId"
        }
      },
      "indexGoldenRecord": "@index(exact)"
    },
    {
      "fieldName": "my_golden_id_b",
      "fieldType": "String",
      "source": {
        "generate": {
          "func": "AppUtils::autoGenerateId"
        }
      },
      "indexGoldenRecord": "@index(exact)"
    }
  ],
  "fieldsForKafkaKeyGen": [
    "given_name",
    "family_name"
  ],
  "nameFieldsForNotificationDisplay": [
    "given_name",
    "family_name"
  ],
  "rules": {
    "link": {
      "deterministic": [
        {
          "vars": [
            "national_id"
          ],
          "text": "eq(national_id)"
        },
        {
          "vars": [
            "given_name",
            "family_name",
            "phone_number"
          ],
          "text": "eq(given_name) and eq(family_name) and eq(phone_number)"
        }
      ]
    }
  }
}
